    \"spectral_bandwidth\": spectral_bandwidth,\n","        \"rolloff\": rolloff,\n","        \"zero_crossing_rate\": zero_crossing_rate,\n","        \"tempo\": tempo,\n","        \"mfccs\": mfccs_str,  # merged MFCCs here\n","    }\n","    return features\n","\n","def dataset_to_csv(dataset_path, output_csv=\"music_genre_features.csv\"):\n","    \"\"\"\n","    Iterates through subfolders (each genre), extracts features for all songs,\n","    and saves everything into one CSV.\n","    \"\"\"\n","    all_features = []\n","\n","    genres = [d for d in os.listdir(dataset_path) if os.path.isdir(os.path.join(dataset_path, d))]\n","\n","    for genre in genres:\n","        genre_path = os.path.join(dataset_path, genre)\n","        print(f\"\\nğŸ§ Processing genre: {genre}\")\n","\n","        for file_name in tqdm(os.listdir(genre_path), desc=f\"Processing {genre}\"):\n","            if file_name.lower().endswith((\".mp3\", \".wav\")):\n","                audio_path = os.path.join(genre_path, file_name)\n","                feats = extract_features(audio_path, genre)\n","                if feats:\n","                    all_features.append(feats)\n","\n","    if all_features:\n","        df = pd.DataFrame(all_features)\n","        df.to_csv(output_csv, index=False)\n","        print(f\"\\nâœ… Extracted features for {len(all_features)} songs.\")\n","        print(f\"ğŸ’¾ Saved to '{output_csv}'\")\n","    else:\n","        print(\"âš ï¸ No valid audio files found.\")\n","\n","if __name__ == \"__main__\":\n","    # Example folder structure:\n","    # dataset/\n","    # â”œâ”€â”€ rock/\n","    # â”‚   â”œâ”€â”€ song1.mp3\n","    # â”‚   â”œâ”€â”€ song2.wav\n","    # â”œâ”€â”€ jazz/\n","    # â”‚   â”œâ”€â”€ song3.mp3\n","    # â”‚   â”œâ”€â”€ song4.wav\n","\n","    dataset_path = \"/content/SONGS\"  # Change this path to your dataset folder\n","    if not os.path.exists(dataset_path):\n","        print(f\"âŒ Folder not found: {dataset_path}\")\n","    else:\n","        datase